{
  "name": "precusor-react",
  "version": "1.0.0",
  "description": "create a react page to display 4-lines precusor results",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && copy src\\pre.html dist\\pre.html && webpack-dev-server --content-base src/ --inline --hot",
    "build:prod": "webpack -p && cp src\\pre.html dist\\pre.html"
  },
  "author": "Yi-Wei Chang",
  "license": "MIT",
  "dependencies": {
    "moment": "^2.22.0",
    "npm": "^5.8.0",
    "react": "^16.2.0",
    "react-datepicker": "^1.4.1",
    "react-dom": "^16.2.0",
    "react-image-lightbox": "^5.0.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1"
  }
}
